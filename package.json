{
	"name": "@felibrary/vue3-ui",
	"version": "0.0.6",
	"description": "vue3 ui组件库",
	"keywords": [
		"ui",
		"vue3",
		"vue",
		"ui-components",
		"components",
		"lib",
		"library"
	],
	"author": "ddzy <1766083035@qq.com>",
	"homepage": "https://github.com/ddzy/vue3-ui#readme",
	"license": "MIT",
	"files": [
		"dist/*"
	],
	"main": "./dist/index.umd.js",
	"module": "./dist/index.mjs",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.umd.js",
			"types": "./dist/typings/index.d.ts"
		},
		"./dist/": {
			"import": "./dist/",
			"require": "./dist/"
		}
	},
	"types": "./dist/typings/index.d.ts",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"serve": "vitepress dev docs",
		"preview": "vitepress preview docs",
		"build:docs": "vitepress build docs",
		"build:lib": "cross-env NODE_OPTIONS=--max-old-space-size=10240 ts-node ./build/lib.build.ts",
		"test": "jest --coverage",
		"commit": "git-cz",
		"format": "prettier-eslint \"packages/**/*.{js,ts,vue,jsx,tsx}\" --config prettier.config.js --write",
		"prepare": "husky install",
		"tree": "tree -L 3 -I 'node_modules|coverage|dist|docs' -o ./tree.txt --dirsfirst",
		"clear:dstore": "find . -name '.DS_Store' | xargs rm -rf"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-conventional-changelog",
			"disableScopeLowerCase": true,
			"disableSubjectLowerCase": true,
			"maxHeaderWidth": 200,
			"maxLineWidth": 200
		}
	},
	"lint-staged": {
		"packages/**/*.{ts,tsx,js,jsx,vue}": [
			"pnpm format"
		]
	},
	"dependencies": {
		"@icon-park/vue-next": "1.4.2",
		"@vueuse/core": "10.9.0",
		"@vueuse/integrations": "10.9.0",
		"change-case": "4.1.2",
		"decimal.js": "10.3.1",
		"tippy.js": "6.3.7",
		"vue": "3.4.27",
		"vue-tippy": "v6"
	},
	"devDependencies": {
		"@types/fs-extra": "11.0.1",
		"@types/jest": "29.5.12",
		"@types/node": "14.14.37",
		"@vitejs/plugin-vue": "4.1.0",
		"@vue/compiler-sfc": "3.4.27",
		"@vue/component-compiler-utils": "3.3.0",
		"@vue/test-utils": "2.4.6",
		"@vue/vue3-jest": "29.2.6",
		"commitizen": "4.2.3",
		"cross-env": "7.0.3",
		"cz-conventional-changelog": "3.3.0",
		"fs-extra": "11.1.1",
		"husky": "6.0.0",
		"identity-obj-proxy": "3.0.0",
		"jest": "29.7.0",
		"jest-environment-jsdom": "29.7.0",
		"jest-jasmine2": "29.7.0",
		"lint-staged": "10.5.4",
		"postcss": "8.4.38",
		"prettier-eslint-cli": "7.1.0",
		"regenerator-runtime": "0.13.9",
		"sass": "1.62.0",
		"terser": "5.16.9",
		"ts-jest": "29.1.2",
		"ts-node": "10.9.2",
		"typescript": "5.0.4",
		"vite": "4.2.1",
		"vite-plugin-css-injected-by-js": "3.1.0",
		"vitepress": "1.0.1",
		"vitepress-theme-demoblock": "3.0.5",
		"vue-tsc": "1.2.0"
	},
	"pnpm": {
		"patchedDependencies": {
			"vitepress-theme-demoblock@3.0.5": "patches/vitepress-theme-demoblock@3.0.5.patch"
		}
	}
}
